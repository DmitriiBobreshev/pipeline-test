{
  "name": "react-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^7.0.3",
    "json-server": "^1.0.0-beta.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "start-server-and-test": "^2.0.5"
  },
  "scripts": {
    "mockservers": "json-server --port 8085 --watch db.json",
    "itest:start": "webpack serve --mode development --open",
    "itest:backend": "start-test mockservers http-get://localhost:8085 itest:start",
    "itest:tests": "echo 'Running tests...'",
    "itest:ci": "cross-env START_SERVER_AND_TEST_INSECURE=1 debug=start-server-and-test start-test itest:backend https-get://localhost:8080 itest:tests",
    "itest:start-ci": "cross-env START_SERVER_AND_TEST_INSECURE=1 debug=start-server-and-test start-test itest:start https-get://localhost:8080 itest:tests"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
